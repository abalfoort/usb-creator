#: usr/lib/usb-creator/usbcreator.py:76
#: usr/share/applications/usb-creator.desktop.in.h:1
msgid "USB Creator"
msgstr "USB-pen skaber"

#: usr/lib/usb-creator/usbcreator.py:77
msgid "Device"
msgstr "Enhed"

#: usr/lib/usb-creator/usbcreator.py:78
msgid "Partition device"
msgstr "Partitionér enhed"

#: usr/lib/usb-creator/usbcreator.py:79
msgid "This will remove all data from the device."
msgstr "Dette vil slette data på enheden."

#: usr/lib/usb-creator/usbcreator.py:80
msgid "USB"
msgstr "USB"

#: usr/lib/usb-creator/usbcreator.py:81
msgid "Available"
msgstr "Tilgængelig"

#: usr/lib/usb-creator/usbcreator.py:82
msgid "Required"
msgstr "Påkrævet"

#: usr/lib/usb-creator/usbcreator.py:83
msgid "Execute"
msgstr "Udfør"

#: usr/lib/usb-creator/usbcreator.py:84
msgid "ISO"
msgstr "ISO"

#: usr/lib/usb-creator/usbcreator.py:85
msgid "Write single ISO"
msgstr "Skriv enkelt ISO"

#: usr/lib/usb-creator/usbcreator.py:86
msgid "Write a single ISO to USB using dd"
msgstr "Skriv en enkelt ISO til USB vha. dd"

#: usr/lib/usb-creator/usbcreator.py:87
msgid "Remove"
msgstr "Slet"

#: usr/lib/usb-creator/usbcreator.py:88
msgid "Refresh device list"
msgstr "Opdatér liste over enheder"

#: usr/lib/usb-creator/usbcreator.py:89
msgid "Unmount device"
msgstr "Afmontér enhed"

#: usr/lib/usb-creator/usbcreator.py:90
msgid "Browse for ISO file"
msgstr "Søg efter IOS-fil"

#: usr/lib/usb-creator/usbcreator.py:91
msgid "Clear the ISO field"
msgstr "Ryd ISO-feltet"

#: usr/lib/usb-creator/usbcreator.py:92
msgid "Manual"
msgstr "Tilpasset"

#: usr/lib/usb-creator/usbcreator.py:93
msgid ""
"Manually select the distribution name if USB Creator cannot\n"
"determine the distribution name automatically."
msgstr ""
"Vælg distributionsnavnet manuelt i tilfælde af at USB-pen skaberen\n"
"ikke selv kan fastslå det."

#: usr/lib/usb-creator/usbcreator.py:97
msgid "Copy ISO to USB..."
msgstr "Kopierer ISO til USB..."

#: usr/lib/usb-creator/usbcreator.py:101
msgid "Prepare copy of ISO..."
msgstr "Forbereder kopiering af ISO..."

#: usr/lib/usb-creator/usbcreator.py:102
msgid "Verify hash of ISO..."
msgstr "Kontrollerer ISO kontrolsum..."

#: usr/lib/usb-creator/usbcreator.py:103
msgid "Install Grub..."
msgstr "Installerer Grub..."

#: usr/lib/usb-creator/usbcreator.py:104
msgid "Unpacking ISO..."
msgstr "Pakker ISO'en ud..."

#: usr/lib/usb-creator/usbcreator.py:105
msgid "Gather ISO information..."
msgstr "Indsamler information om ISO'en..."

#: usr/lib/usb-creator/usbcreator.py:149
msgid "Version"
msgstr "Version"

#: usr/lib/usb-creator/usbcreator.py:164
msgid ""
"Cannot add ISO from path: {}.\n"
"Please, remove the ISO path or browse for an existing ISO."
msgstr ""
"Kan ikke tilføje ISO fra stien:{}.\n"
"Ryd venligst ISO-stien eller gennemse systemet for at finde en eksisterende "
"ISO."

#: usr/lib/usb-creator/usbcreator.py:172
msgid ""
"There is not enough space available on the pen drive.\n"
"Please, remove unneeded files before continuing."
msgstr ""
"Der er ikke tilstrækkelig plads på USB-pennen.\n"
"Fjern venligst unødvendige filer før du fortsætter."

#: usr/lib/usb-creator/usbcreator.py:201
msgid "Are you sure you want to remove the selected ISO from the device?"
msgstr "Er du sikker på at du vil fjerne den valgte ISO fra enheden?"

#: usr/lib/usb-creator/usbcreator.py:222
msgid "Select ISO"
msgstr "Vælg ISO"

#: usr/lib/usb-creator/usbcreator.py:279
msgid "Unmount"
msgstr "Afmontér"

#: usr/lib/usb-creator/usbcreator.py:284
msgid "You can now safely remove the device."
msgstr "Du kan nu fjerne enheden."

#: usr/lib/usb-creator/usbcreator.py:286
msgid ""
"Could not unmount the device.\n"
"Please unmount the device manually."
msgstr ""
"Kan ikke afmontere enheden.\n"
"Afmontér venligst manuelt."

#: usr/lib/usb-creator/usbcreator.py:324
msgid ""
"No partition found.\n"
"Please format the USB before you continue."
msgstr ""
"Der blev ikke fundet en partition.\n"
"Formatér venligst USB-pennen før du fortsætter."

#: usr/lib/usb-creator/usbcreator.py:326
msgid "No Partition"
msgstr "Ingen Partition"

#: usr/lib/usb-creator/usbcreator.py:563
msgid "Error"
msgstr "Fejl"

#: usr/lib/usb-creator/usbcreator.py:566
msgid "Wrong arguments were passed."
msgstr "Der blev anvendt ugyldige argumenter."

#: usr/lib/usb-creator/usbcreator.py:568
msgid "The device was not found."
msgstr "Enheden kunne ikke findes."

#: usr/lib/usb-creator/usbcreator.py:570
msgid "The device is not detachable."
msgstr "Enheden kan ikke frakobles."

#: usr/lib/usb-creator/usbcreator.py:572
msgid "The device has no partition."
msgstr "Der findes ingen partition på enheden."

#: usr/lib/usb-creator/usbcreator.py:574
msgid "Unable to mount the device."
msgstr "Kunne ikke afmontere enheden."

#: usr/lib/usb-creator/usbcreator.py:576
msgid "Given ISO path was not found."
msgstr "Angivet ISO-sti kunne ikke findes."

#: usr/lib/usb-creator/usbcreator.py:578
msgid ""
"ISO too large for FAT formatted USB (max 4GB).\n"
"Format the USB to exFAT, NTFS, ext4, etc."
msgstr ""
"ISO'en er for stor til en FAT-formateret USB-pen(max 4GB).\n"
"Formatér i stedet USB-pennen med exFAT, NTFS, ext4 e.l."

#: usr/lib/usb-creator/usbcreator.py:581
msgid "There is not enough space available on the device."
msgstr "Der er ikke tilstrækkelig ledig plads på enheden."

#: usr/lib/usb-creator/usbcreator.py:583
msgid ""
"Unable to determine the distribution name.\n"
"Select a distribution in the 'Manual' section."
msgstr ""
"Det lykkedes ikke at fastslå navnet på distributionen.\n"
"Vælg et distributionsnavn under 'Tilpasset'."

#: usr/lib/usb-creator/usbcreator.py:586
msgid "Cannot find the target ISO file. Did you insert the USB?"
msgstr "Destinations-ISO'en kunne ikke findes. Har du indsat en USB-pen?"

#: usr/lib/usb-creator/usbcreator.py:588
msgid "Hash mismatch."
msgstr "Forkert kontrolsum."

#: usr/lib/usb-creator/usbcreator.py:590
msgid "Copy of ISO failed."
msgstr "ISO-kopiering mislykkedes."

#: usr/lib/usb-creator/usbcreator.py:592
msgid "Device is in use by another application."
msgstr "Enheden bruges af et andet program lige nu."

#: usr/lib/usb-creator/usbcreator.py:594
msgid ""
"An unknown error accured.\n"
"Please, visit our forum for support: https://forums.solydxk.com"
msgstr ""
"En ukendt fejl er opstået.\n"
"Besøg venligst vores forum for yderligere hjælp: https://forums.solydxk.com"

#: usr/lib/usb-creator/usbcreator.py:598
msgid "The USB was successfully written."
msgstr "USB-pennen blev færdiggjort korrekt."

#: usr/lib/usb-creator/usbcreator.py:599
msgid "Finished"
msgstr "Færdig"

#: usr/lib/usb-creator/main.py:49
msgid "Unexpected error"
msgstr "Uventet fejl"

#: usr/lib/usb-creator/main.py:50
msgid ""
"USB Creator has failed with the following unexpected error. Please submit a "
"bug report!"
msgstr ""
"USB-skaberen er fejlet med følgende uventede fejl. Insend venligst en "
"fejrapport!"

#: usr/share/applications/usb-creator.desktop.in.h:2
msgid "Create a multi-boot USB"
msgstr "Skab en multi-boot USB-pen"

#: usr/share/solid/actions/usb-constructor-openusb.desktop.in.h:1
msgid "Create a multi-boot USB with the USB Creator"
msgstr "Skab en multi-boot USB-pen ved hjælp af USB-skaberen"

#. (itstool) path: action/message
#: usr/share/polkit-1/actions/com.solydxk.pkexec.usb-creator.tmp-sh.policy:8
msgid "USB Creator requires authentication to install Grub on your device"
msgstr ""
"USB skaberen kræver autorisation for at kunne installere Grub på enheden"

#: po/en/help.html%2Bhtml.body.h1:8-2
msgid "USB Creator Help"
msgstr "Hjælp til USB-skaber"

#: po/en/help.html%2Bhtml.body.div.h2:10-3
msgid "Description"
msgstr "Beskrivelse"

#: po/en/help.html%2Bhtml.body.div.p:11-3
msgid ""
"The USB Creator is a tool to help users to create a bootable flash drive "
"with multiple Linux operating systems."
msgstr ""
"USb-skaberen er et værktøj der gør det muligt for brugere at lave en USB-pen "
"hvorfra man kan starte forskellige udgave af Linux."

#: po/en/help.html%2Bhtml.body.div.h2:13-3
msgid "Important"
msgstr "Vigtigt"

#: po/en/help.html%2Bhtml.body.div.p:14-3
msgid ""
"Make sure you backed up all important data from the flash drive before you "
"start."
msgstr ""
"Kontrollér at du har sikkerhedskopieret alle vigtige data fra USB-pennen for "
"du begynder."

#: po/en/help.html%2Bhtml.body.div.p:15-3
msgid ""
"USB Creator expects a USB pen drive with a single fat32 partition or a 100MB "
"fat32 boot partition and a second partition for the ISOs in the file system "
"format of your choosing."
msgstr ""
"USB skaberen forventer at finde en USB-pen med en enkelt fat32 partition "
"ELLER en 100MB fat32 opstartspartition OG yderligere en partition til plads "
"til ISO-filerne, formateret med dit foretrukne filsystem."

#: po/en/help.html%2Bhtml.body.div.p:16-3
msgid "Note: a fat32 partition can not hold files larger than 4GB."
msgstr "Bemærk: En fat32 partition kan ikke håndtere filer større end 4GB."

#: po/en/help.html%2Bhtml.body.div.h2:18-3
msgid "Terminal"
msgstr "Terminalvindue"

#: po/en/help.html%2Bhtml.body.div.p:19-3
msgid "You can also use the USB Creator from the terminal."
msgstr "Du kan også anvende USB skaberen i et terminalvindue."

#: po/en/help.html%2Bhtml.body.div.p:20-3
msgid "See \"usb-creator -h\" for a list of all available options."
msgstr "Kør \"usb-creator -h\" for at få en liste over valgmulighederne. "

#: po/en/help.html%2Bhtml.body.div.h2:22-3
msgid "Your ISO is not booting?"
msgstr "Kan du ikke starte op fra din ISO?"

#: po/en/help.html%2Bhtml.body.div.p:23-3
msgid ""
"If the ISO did not boot with automatic distribution name detection, you "
"manually select the appropriate distribution name from the \"Manual\" drop "
"down menu."
msgstr ""
"Hvis ISO'en ikke kunne startes op når distributionsnavnet blev valgt "
"automatisk, bør du selv vælge distributionsnavn under rullegardinmenuen i "
"\"Tilpasset\"."

#: po/en/help.html%2Bhtml.body.div.p:24-3
msgid ""
"If you still cannot boot your ISO you can configure the menu on your flash "
"drive: /boot/grub/grub.cfg"
msgstr ""
"Hvis du stadig ikke kan starte op på ISO'en, kan du tilrette USB-pennens "
"menu: /boot/grub/grub.cfg"

#: po/en/help.html%2Bhtml.body.div.p:25-3
msgid ""
"Please help improve this application by adapting grub.cfg until the ISO "
"boots and post your changes on <a href=\"https://github.com/SolydXK/usb-"
"creator\" target=\"_blank\">GitHub</a> or our <a href=\"https://forums."
"solydxk.com\" target=\"_blank\">forum</a>."
msgstr ""
"Hjælp venligst med at forbedre denne applikation ved at tilpasse grub.cfg "
"indtil ISO'en kan startes op, og beskriv herefter dine rettelser på <a href="
"\"https://github.com/SolydXK/usb-creator\" target=\"_blank\">GitHub</a> "
"eller vores <a href=\"https://forums.solydxk.com\" target=\"_blank\">forum</"
"a>."

#: po/en/help.html%2Bhtml.body.div.p:26-3
msgid ""
"Don't forget to attach the log file when you post an issue on our forum: ~/."
"usb-creator/usb-creator.log"
msgstr ""
"Glem ikke at vedhæfte logfilen hvis du opretter en sag i vores supportforum. "
"Filen findes her: ~/.usb-creator/usb-creator.log"
